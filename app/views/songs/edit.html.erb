<h1>Edit a Song</h1>
<%= form_for(@song) do |f|  %>
  <%= f.label :name %>
  <%= f.text_field :name %><br />
  <%= f.label :artist_id %>
  <%= f.number_field :artist_id %><br />
  <%= f.label :genre_id %>
  <%= f.number_field :genre_id %><br />
  <br />
  <%= f.submit %>
<% end %>


